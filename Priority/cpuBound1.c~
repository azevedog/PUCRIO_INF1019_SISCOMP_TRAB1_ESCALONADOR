//Senha.
#define PASS "abcdefghijlmnopqrstuvxz"

//Tamanho da senha
#define SIZE 23

// Funcao principal de trabalho. 42 indica conclusao com sucesso.
int main (void){
	int i;
	char try;

	for(i=0; i<SIZE; i++){
		try = 'a';
		while(PASS[i]!=try) try++;
	}
	return 42;
}
